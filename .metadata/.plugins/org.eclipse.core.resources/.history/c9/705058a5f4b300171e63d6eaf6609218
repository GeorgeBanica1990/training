package testclasses;

public class ResourceClass implements ResourceInterface{

	private int id;
	private String text;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

	public String showResource(int id, String text) {
		return text + id;
	}

}
